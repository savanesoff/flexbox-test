{
  "name": "cobalt-preact-ts-webpack-swc-tailwindcss",
  "version": "1.0.0",
  "description": "Cobalt example project for React with TypeScript, Webpack, SWC, Tailwind CSS ",
  "main": "./dist/main.js",
  "type": "module",
  "scripts": {
    "start": "pnpm generate:tailwind && pnpm generate:fonts && pnpm webpack serve --mode development --hot",
    "build": "pnpm generate:tailwind && generate:fonts && webpack --mode production",
    "lint": "eslint . --ignore-pattern dist/",
    "format": "prettier --write .",
    "test": "vitest",
    "generate:tailwind": "node --experimental-specifier-resolution=node ./src/tailwindcss/scripts/generate-tailwind-scss.js",
    "generate:fonts": "node --experimental-specifier-resolution=node ./src/tailwindcss/scripts/generate-fonts.js && pnpm format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "clsx": "^2.1.1",
    "core-js": "^3.37.1",
    "preact": "10.22.0",
    "regenerator-runtime": "^0.14.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-syntax-import-assertions": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.4.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.19",
    "css-loader": "^7.1.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-parser": "link:@types/@babel/eslint-parser",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.38",
    "postcss-custom-properties": "^13.3.10",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.5.13",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
